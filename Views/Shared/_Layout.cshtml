                                    <img src="~/img/JacqBBrownBG.png" >
                                
                            </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-12">
                            <ul class="mainmenu justify-content-lg-center justify-content-start">
                                <li>
                                        <a class="hover-flip-item-wrapper" href="mailto:veparity@gmail.com">
                                        <span class="hover-flip-item">
                                            <span data-text="Let's Get in Touch">Let's Get in Touch</span>
                                        </span>
                                    </a>
                                </li>
                            
                          
                            </ul>
